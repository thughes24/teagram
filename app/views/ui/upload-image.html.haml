.row
  .col-md-12.text-center
    %h4 Upload Progress
    .progress
      .progress-bar.progress-bar-striped.progress-bar-success.active{'aria-valuenow' => '30','aria-valuemin' => '0', 'aria-valuemax' => '100', style: 'width:30%;'}
        30%
.row
  .col-md-8.col-centered.text-center.vertical-center
    %form.form-vertical
      .form-group
        %label.control-label{for: "image"} Image:
        %input.form-control{type: "file", name: "image"}
      %input.btn.btn-primary{type: 'submit'}


